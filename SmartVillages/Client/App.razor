<Router AppAssembly="@typeof(Program).Assembly" PreferExactMatches="@true">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(UserSignLayout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(EmptyLayout)">
            <SmartVillages.Client.Pages.NotFound></SmartVillages.Client.Pages.NotFound>
        </LayoutView>
    </NotFound>
</Router>
